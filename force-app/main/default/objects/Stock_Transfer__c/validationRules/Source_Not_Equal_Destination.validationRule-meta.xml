<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/metadata">
    <fullName>Source_Not_Equal_Destination</fullName>
    <active>true</active>
    <errorConditionFormula>Source_Warehouse__c = Destination_Warehouse__c</errorConditionFormula>
    <errorMessage>Source and destination warehouses must be different.</errorMessage>
</ValidationRule>
