<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/metadata">
    <fullName>Quantity_Positive</fullName>
    <active>true</active>
    <description>Transaction quantity must be positive</description>
    <errorConditionFormula>Quantity__c &lt;= 0</errorConditionFormula>
    <errorMessage>Transaction quantity must be greater than zero.</errorMessage>
</ValidationRule>
